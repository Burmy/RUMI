pend(\"smoking\", document.querySelector('input[name=\"smoke\"]:checked').value);\n                                    form.append(\"gender\", document.querySelector('input[name=\"gender\"]:checked').value);\n                                    form.append(\"creator_id\", 1);\n                                    form.append(\"photo\", photo.files[0]);\n\n                                    console.log(\n                                        form.getAll(\"caption\"),\n                                        form.getAll(\"description\"),\n                                        form.getAll(\"price\"),\n                                        form.getAll(\"location\"),\n                                        form.getAll(\"parking\"),\n                                        form.getAll(\"pet\"),\n                                        form.getAll(\"smoking\"),\n                                        form.getAll(\"gender\"),\n                                        form.getAll(\"creator_id\"),\n                                        form.getAll(\"photo\")\n                                    );\n                                    Axios.post(\"http://18.190.48.206:3001/posts/\", form, {\n                                        headers: { \"content-type\": \"multipart/form-data\" },\n                                    })\n                                        .then((result) => {\n                                            console.log(result);\n                                            alert(\"Successfully Posted\");\n                                        })\n                                        .catch((error) => {\n                                            console.log(error.response);\n                                        });\n                                }\n                            }}\n                        >\n                            Submit\n                        </button>\n                    </div>\n                </div>\n         